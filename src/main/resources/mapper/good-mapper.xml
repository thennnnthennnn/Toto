<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.p.Dao.GoodMapper">
    <insert id="insertGood">
        insert into goods(price,name,inventory)values(#{price},#{name},#{inventory})
    </insert>
    <select id="selectGood" resultType="com.p.Model.Good">
        select*from goods where id=#{id}
    </select>
    <delete id="deleteGood">
        delete from goods where id=#{id}
    </delete>
    <update id="updateGood">
        update goods set price = #{price},name =#{name},inventory=#{inventory} where id=#{id}
    </update>
</mapper>